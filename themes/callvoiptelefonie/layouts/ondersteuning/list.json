[{{ range $index, $page := where .Data.Pages.ByWeight "Type" "ondersteuning" }}
  {{- if ne $page.Type "json" -}}
  {{- if and $index (gt $index 0) -}},{{- end }}
  {
    "uri": "{{ $page.Permalink }}",
    "title": "{{ htmlEscape $page.Title}}",
    "description": "{{ htmlEscape $page.Description}}"
  }
  {{- end -}}
  {{- end -}}]